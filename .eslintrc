{
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "extends": ["airbnb", "react-app", "prettier", "prettier/react"],
  "rules": {
  "react/jsx-filename-extension": 0,
  "react/prefer-stateless-function": 0,
  "react/jsx-one-expression-per-line": "off",
  "prettier/prettier": ["error"]
  },
  "env": {
  "es6": true
  }
}